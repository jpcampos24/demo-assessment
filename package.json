  "dependencies": {
    "cypress": "^10.9.0",
    "cypress-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.2.1",
    "@cypress/xpath": "^2.0.2",
